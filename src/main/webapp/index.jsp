        <script>
            // Preloader Logic
            window.addEventListener('load', () => {
                const preloader = document.getElementById('preloader');
                // Minimum display time of 1.5s to show the animation
                setTimeout(() => {
                    preloader.classList.add('fade-out');
                    setTimeout(() => {
                        preloader.style.display = 'none';
                    }, 600);
                }, 1500);
            });

            // Navbar Scroll
            const navbar = document.getElementById('navbar');

            // === UNIFIED SCROLL HANDLER (performance optimized) ===
            const scrollProgressEl = document.getElementById('scrollProgress');
            const sections = document.querySelectorAll('section[id]');
            const navLinks = document.querySelectorAll('.nav-link');
            var backToTopBtn = document.getElementById('backToTop');
            let scrollTicking = false;

            function onScroll() {
                const scrollY = window.scrollY;
                const scrollHeight = document.documentElement.scrollHeight - document.documentElement.clientHeight;

                // Navbar
                navbar.classList.toggle('scrolled', scrollY > 50);

                // Scroll Progress
                scrollProgressEl.style.width = (scrollY / scrollHeight * 100) + '%';

                // Scroll Spy
                let current = '';
                sections.forEach(function (section) {
                    if (scrollY >= section.offsetTop - 120) {
                        current = section.getAttribute('id');
                    }
                });
                navLinks.forEach(function (link) {
                    link.classList.remove('active');
                    if (link.getAttribute('href') === '#' + current) {
                        link.classList.add('active');
                    }
                });

                // Back to Top
                if (scrollY > 500) {
                    backToTopBtn.classList.add('visible');
                } else {
                    backToTopBtn.classList.remove('visible');
                }

                scrollTicking = false;
            }

            window.addEventListener('scroll', function () {
                if (!scrollTicking) {
                    requestAnimationFrame(onScroll);
                    scrollTicking = true;
                }
            }, { passive: true });

            backToTopBtn.addEventListener('click', function () {
                window.scrollTo({ top: 0, behavior: 'smooth' });
            });

            // Mobile Menu
            const navToggle = document.getElementById('navToggle');
            const navMenu = document.getElementById('navMenu');
            navToggle.addEventListener('click', () => {
                navMenu.classList.toggle('active');
            });

            // Smooth Scroll
            document.querySelectorAll('a[href^="#"]').forEach(anchor => {
                anchor.addEventListener('click', function (e) {
                    e.preventDefault();
                    const target = document.querySelector(this.getAttribute('href'));
                    if (target) {
                        target.scrollIntoView({ behavior: 'smooth' });
                        navMenu.classList.remove('active');
                    }
                });
            });

            // Countdown Timer
            function updateCountdown() {
                const eventDate = new Date('March 11, 2026 09:00:00').getTime();
                const now = new Date().getTime();
                const diff = eventDate - now;

                if (diff > 0) {
                    document.getElementById('days').textContent = String(Math.floor(diff / (1000 * 60 * 60 * 24))).padStart(2, '0');
                    document.getElementById('hours').textContent = String(Math.floor((diff % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60))).padStart(2, '0');
                    document.getElementById('minutes').textContent = String(Math.floor((diff % (1000 * 60 * 60)) / (1000 * 60))).padStart(2, '0');
                    document.getElementById('seconds').textContent = String(Math.floor((diff % (1000 * 60)) / 1000)).padStart(2, '0');
                }
            }
            updateCountdown();
            setInterval(updateCountdown, 1000);

            // Scroll Reveal
            const revealElements = document.querySelectorAll('.reveal');
            const revealObserver = new IntersectionObserver((entries) => {
                entries.forEach(entry => {
                    if (entry.isIntersecting) {
                        entry.target.classList.add('visible');
                    }
                });
            }, { threshold: 0.1, rootMargin: '-50px' });
            revealElements.forEach(el => revealObserver.observe(el));

            // Event Filter
            const filterBtns = document.querySelectorAll('.filter-btn');
            const eventCards = document.querySelectorAll('.event-card');

            filterBtns.forEach(btn => {
                btn.addEventListener('click', () => {
                    filterBtns.forEach(b => b.classList.remove('active'));
                    btn.classList.add('active');
                    const filter = btn.dataset.filter;

                    eventCards.forEach(card => {
                        if (filter === 'all' || card.dataset.category === filter) {
                            card.style.display = 'block';
                        } else {
                            card.style.display = 'none';
                        }
                    });
                });
            });

            // FAQ Accordion
            document.querySelectorAll('.faq-item').forEach(item => {
                const question = item.querySelector('.faq-question');
                question.addEventListener('click', () => {
                    document.querySelectorAll('.faq-item').forEach(i => {
                        if (i !== item) i.classList.remove('active');
                    });
                    item.classList.toggle('active');
                });
            });

            // Event Card Stagger Animation
            var eventCardsList = document.querySelectorAll('.event-card.reveal');
            var staggerObserver = new IntersectionObserver(function (entries) {
                entries.forEach(function (entry) {
                    if (entry.isIntersecting) {
                        var cards = entry.target.parentElement.querySelectorAll('.event-card');
                        cards.forEach(function (card, i) {
                            card.style.transitionDelay = (i * 0.1) + 's';
                            card.classList.add('visible');
                        });
                        staggerObserver.unobserve(entry.target);
                    }
                });
            }, { threshold: 0.1 });
            if (eventCardsList.length > 0) {
                staggerObserver.observe(eventCardsList[0]);
            }

            // Schedule Tab Switching
            document.querySelectorAll('.schedule-tab').forEach(function (tab) {
                tab.addEventListener('click', function () {
                    document.querySelectorAll('.schedule-tab').forEach(function (t) { t.classList.remove('active'); });
                    tab.classList.add('active');
                    var day = tab.dataset.day;
                    document.getElementById('scheduleDay1').style.display = day === '1' ? 'block' : 'none';
                    document.getElementById('scheduleDay2').style.display = day === '2' ? 'block' : 'none';
                    // Re-observe reveal elements in shown timeline
                    var shownTimeline = document.getElementById('scheduleDay' + day);
                    shownTimeline.querySelectorAll('.reveal').forEach(function (el) {
                        el.classList.remove('visible');
                        revealObserver.observe(el);
                    });
                });
            });

            // Faculty Carousel
            (function () {
                const track = document.querySelector('.faculty-track');
                const wrapper = document.querySelector('.faculty-track-wrapper');
                const prevBtn = document.querySelector('.faculty-prev');
                const nextBtn = document.querySelector('.faculty-next');
                if (!track || !prevBtn || !nextBtn || !wrapper) return;

                const cards = track.querySelectorAll('.faculty-card');
                let currentIndex = 0;
                const gap = 20; // 1.25rem

                function getVisibleCount() {
                    if (window.innerWidth <= 480) return 1;
                    if (window.innerWidth <= 768) return 2;
                    return 3;
                }

                function getMaxIndex() {
                    return Math.max(0, cards.length - getVisibleCount());
                }

                function updateCarousel() {
                    const visible = getVisibleCount();
                    const cardWidth = (track.parentElement.offsetWidth - gap * (visible - 1)) / visible;
                    const offset = currentIndex * (cardWidth + gap);
                    track.style.transform = 'translateX(-' + offset + 'px)';
                    prevBtn.disabled = currentIndex <= 0;
                    nextBtn.disabled = currentIndex >= getMaxIndex();
                }

                prevBtn.addEventListener('click', function () {
                    if (currentIndex > 0) {
                        currentIndex--;
                        updateCarousel();
                    }
                });

                nextBtn.addEventListener('click', function () {
                    if (currentIndex < getMaxIndex()) {
                        currentIndex++;
                        updateCarousel();
                    }
                });

                // Mouse drag scrolling
                let isDragging = false;
                let startX = 0;
                let dragDelta = 0;

                wrapper.addEventListener('mousedown', function (e) {
                    isDragging = true;
                    startX = e.clientX;
                    dragDelta = 0;
                    track.style.transition = 'none';
                    wrapper.style.cursor = 'grabbing';
                    e.preventDefault();
                });

                document.addEventListener('mousemove', function (e) {
                    if (!isDragging) return;
                    dragDelta = e.clientX - startX;
                    const visible = getVisibleCount();
                    const cardWidth = (track.parentElement.offsetWidth - gap * (visible - 1)) / visible;
                    const baseOffset = currentIndex * (cardWidth + gap);
                    track.style.transform = 'translateX(' + (-baseOffset + dragDelta) + 'px)';
                });

                document.addEventListener('mouseup', function () {
                    if (!isDragging) return;
                    isDragging = false;
                    track.style.transition = '';
                    wrapper.style.cursor = '';
                    const threshold = 60;
                    if (dragDelta < -threshold && currentIndex < getMaxIndex()) {
                        currentIndex++;
                    } else if (dragDelta > threshold && currentIndex > 0) {
                        currentIndex--;
                    }
                    updateCarousel();
                });

                // Touch swipe support
                let touchStartX = 0;
                let touchDelta = 0;

                wrapper.addEventListener('touchstart', function (e) {
                    touchStartX = e.touches[0].clientX;
                    touchDelta = 0;
                    track.style.transition = 'none';
                }, { passive: true });

                wrapper.addEventListener('touchmove', function (e) {
                    touchDelta = e.touches[0].clientX - touchStartX;
                    const visible = getVisibleCount();
                    const cardWidth = (track.parentElement.offsetWidth - gap * (visible - 1)) / visible;
                    const baseOffset = currentIndex * (cardWidth + gap);
                    track.style.transform = 'translateX(' + (-baseOffset + touchDelta) + 'px)';
                }, { passive: true });

                wrapper.addEventListener('touchend', function () {
                    track.style.transition = '';
                    const threshold = 50;
                    if (touchDelta < -threshold && currentIndex < getMaxIndex()) {
                        currentIndex++;
                    } else if (touchDelta > threshold && currentIndex > 0) {
                        currentIndex--;
                    }
                    updateCarousel();
                });

                window.addEventListener('resize', function () {
                    if (currentIndex > getMaxIndex()) currentIndex = getMaxIndex();
                    updateCarousel();
                });

                // Auto-play: advance every 6 seconds, pause on hover
                var autoplayInterval = setInterval(function () {
                    if (currentIndex < getMaxIndex()) {
                        currentIndex++;
                    } else {
                        currentIndex = 0;
                    }
                    updateCarousel();
                }, 2000);

                wrapper.addEventListener('mouseenter', function () {
                    clearInterval(autoplayInterval);
                });
                wrapper.addEventListener('mouseleave', function () {
                    autoplayInterval = setInterval(function () {
                        if (currentIndex < getMaxIndex()) {
                            currentIndex++;
                        } else {
                            currentIndex = 0;
                        }
                        updateCarousel();
                    }, 2000);
                });

                updateCarousel();
            })();
        </script>

        <script>
            // 3D Tech Globe Animation
            (function () {
                function initTechGlobe() {
                    const container = document.getElementById('techGlobe');
                    if (!container) return;

                    container.innerHTML = '';

                    // 28 tech icons for a richer globe
                    const tags = [
                        'devicon-java-plain colored',
                        'devicon-python-plain colored',
                        'devicon-javascript-plain colored',
                        'devicon-react-original colored',
                        'devicon-html5-plain colored',
                        'devicon-css3-plain colored',
                        'devicon-nodejs-plain colored',
                        'devicon-docker-plain colored',
                        'devicon-git-plain colored',
                        'devicon-mysql-plain colored',
                        'devicon-typescript-plain colored',
                        'devicon-github-original',
                        'devicon-mongodb-plain colored',
                        'devicon-vuejs-plain colored',
                        'devicon-angularjs-plain colored',
                        'devicon-linux-plain colored',
                        'devicon-firebase-plain colored',
                        'devicon-amazonwebservices-original colored',
                        'devicon-flutter-plain colored',
                        'devicon-kotlin-plain colored',
                        'devicon-rust-plain',
                        'devicon-go-original-wordmark colored',
                        'devicon-swift-plain colored',
                        'devicon-redis-plain colored',
                        'devicon-postgresql-plain colored',
                        'devicon-kubernetes-plain colored',
                        'devicon-tensorflow-original colored',
                        'devicon-cplusplus-plain colored'
                    ];

                    const radius = 160;
                    const elements = [];

                    // Create elements
                    tags.forEach((icon, i) => {
                        const el = document.createElement('div');
                        el.className = 'globe-item';
                        el.innerHTML = '<i class="' + icon + '"></i>';
                        container.appendChild(el);

                        // Fibonacci sphere for even distribution
                        const phi = Math.acos(-1 + (2 * i + 1) / tags.length);
                        const theta = Math.sqrt(tags.length * Math.PI) * phi;

                        elements.push({
                            el: el,
                            bx: radius * Math.cos(theta) * Math.sin(phi),
                            by: radius * Math.sin(theta) * Math.sin(phi),
                            bz: radius * Math.cos(phi)
                        });
                    });

                    let angle = 0;
                    const speed = 0.005; // Faster rotation

                    // Optimized animation loop
                    function animate() {
                        angle += speed;
                        const cos = Math.cos(angle);
                        const sin = Math.sin(angle);

                        for (let i = 0; i < elements.length; i++) {
                            const item = elements[i];
                            const x = item.bx * cos + item.bz * sin;
                            const z = -item.bx * sin + item.bz * cos;
                            const scale = 400 / (400 + z);
                            const opacity = 0.4 + (z + radius) / (2 * radius) * 0.6;

                            item.el.style.cssText = 'transform:translate(-50%,-50%)translate3d(' + x.toFixed(0) + 'px,' + item.by.toFixed(0) + 'px,0)scale(' + scale.toFixed(2) + ');opacity:' + opacity.toFixed(1) + ';z-index:' + Math.floor(z + radius);
                        }
                        requestAnimationFrame(animate);
                    }

                    animate();
                }

                setTimeout(initTechGlobe, 150);
            })();
        </script>

        <!-- IT Developer Themed Background Animation -->
        <script>
            // Matrix Code Rain Effect
            (function () {
                const canvas = document.createElement('canvas');
                canvas.id = 'matrixCanvas';
                canvas.style.cssText = 'position:fixed;top:0;left:0;width:100%;height:100%;z-index:0;pointer-events:none;opacity:0.15;';
                document.querySelector('.bg-wrapper').appendChild(canvas);

                const ctx = canvas.getContext('2d');

                function resize() {
                    canvas.width = window.innerWidth;
                    canvas.height = window.innerHeight;
                }
                resize();
                window.addEventListener('resize', resize);

                // Characters: Binary, brackets, operators, tech symbols
                const chars = '01</>{}[]()=+-*&|!?#@$%^~`:;ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz';
                const fontSize = 14;
                const columns = Math.floor(canvas.width / fontSize);
                const drops = [];

                for (let i = 0; i < columns; i++) {
                    drops[i] = Math.random() * -100;
                }

                function draw() {
                    ctx.fillStyle = 'rgba(3, 0, 20, 0.08)';
                    ctx.fillRect(0, 0, canvas.width, canvas.height);

                    for (let i = 0; i < drops.length; i++) {
                        const char = chars[Math.floor(Math.random() * chars.length)];
                        const x = i * fontSize;
                        const y = drops[i] * fontSize;

                        // Gradient from cyan to purple
                        const hue = 180 + Math.sin(Date.now() * 0.001 + i * 0.1) * 60;
                        ctx.fillStyle = `hsla(${hue}, 80%, 60%, ${0.3 + Math.random() * 0.4})`;
                        ctx.font = fontSize + 'px monospace';
                        ctx.fillText(char, x, y);

                        // Bright leading character
                        if (Math.random() > 0.98) {
                            ctx.fillStyle = '#fff';
                            ctx.fillText(char, x, y);
                        }

                        if (y > canvas.height && Math.random() > 0.99) {
                            drops[i] = 0;
                        }
                        drops[i] += 0.5 + Math.random() * 0.5;
                    }

                    setTimeout(function () { requestAnimationFrame(draw); }, 50);
                }
                draw();
            })();

